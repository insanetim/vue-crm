{
  "root": true,
  "extends": ["plugin:vue/vue3-recommended", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-empty": 0,
    "no-undef": 0,
    "vue/html-self-closing": [
      "error",
      {
        "html": {
          "component": "always",
          "normal": "never",
          "void": "always"
        },
        "svg": "always"
      }
    ]
  }
}
